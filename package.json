  "bugs": {
    "url": "https://github.com/maniekes/oh-to-png/issues"
  },
  "homepage": "https://github.com/maniekes/oh-to-png#readme",
  "dependencies": {
    "express": "^4.21.2",
    "js-yaml": "^4.1.0",
    "pageres": "^8.1.0",
    "pm2": "^5.4.3",
    "sharp": "^0.33.5",
    "yargs": "^17.7.2"
  }
}
