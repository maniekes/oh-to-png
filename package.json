{
  "name": "oh-to-png",
  "version": "1.0.0",
  "description": "serve openhab panels as static images for embedded devices",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "watch": "node --watch index.js",
    "stop": "pm2 stop oh-to-png",
    "startup": "pm2 startup",
    "restart": "pm2 restart oh-to-png",
    "delete": "pm2 delete oh-to-png"
  },
  "bin": {
    "oh-to-png": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maniekes/oh-to-png.git"
  },
